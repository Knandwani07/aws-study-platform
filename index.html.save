    <!-- Hero Section -->
    <section id="home" class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <div class="hero-badge">
                    <span class="badge-icon">🚀</span>
                    <span>Accelerate Your Cloud Journey</span>
                </div>
                <h1 class="hero-title">
                    Master AWS.<br>
                    <span class="gradient-text">Build Your Future.</span>
                </h1>
                <p class="hero-description">
                    Transform your career with comprehensive AWS training. From Cloud Practitioner to Solutions Architect - we've got your path mapped out.
                </p>
                <div class="hero-actions">
                    <button class="btn-primary" onclick="startQuiz()">
                        Start Learning
